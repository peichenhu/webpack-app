{
    "name": "webpack-app",
    "description": "本项目用于测试 webpack 构建 vue3 项目，使用了 ts 作为 webpack 的配置语言，配置了一些优化操作，强制依赖包源的版本锁定。",
    "version": "1.0.0",
    "author": "pch1024",
    "license": "ISC",
    "packageManager": "pnpm@8.10.5",
    "sideEffects": true,
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "webpack serve --config ./ts-config/webpack.development.ts",
        "build": "webpack --config ./ts-config/webpack.production.ts",
        "build:dev": "webpack --config ./ts-config/webpack.development.ts",
        "build:analyzer": "webpack --config ./ts-config/webpack.analyzer.ts",
        "build:report": "webpack --config ./ts-config/webpack.report.ts"
    },
    "devDependencies": {
        "@babel/core": "7.23.5",
        "@babel/generator": "7.23.5",
        "@babel/parser": "7.23.5",
        "@babel/preset-env": "7.23.5",
        "@babel/preset-typescript": "7.23.3",
        "@babel/traverse": "7.23.5",
        "@babel/types": "7.23.5",
        "@swc/core": "1.3.100",
        "@types/babel__core": "7.20.5",
        "@types/babel__generator": "7.6.7",
        "@types/babel__traverse": "7.20.4",
        "@types/loader-utils": "2.0.6",
        "@types/lodash-es": "4.17.12",
        "@types/node": "20.10.2",
        "@types/webpack": "5.28.5",
        "@types/webpack-bundle-analyzer": "4.6.3",
        "@types/webpack-sources": "3.2.3",
        "@vue/compiler-sfc": "3.3.9",
        "art-template-loader": "1.4.3",
        "chalk": "5.3.0",
        "compression-webpack-plugin": "10.0.0",
        "consola": "3.2.3",
        "copy-webpack-plugin": "11.0.0",
        "css-loader": "6.8.1",
        "css-minimizer-webpack-plugin": "5.0.1",
        "esbuild-loader": "4.0.2",
        "extract-css-chunks-webpack-plugin": "^4.10.0",
        "html-webpack-plugin": "5.5.3",
        "loader-utils": "3.2.1",
        "mini-css-extract-plugin": "2.7.6",
        "schema-utils": "4.2.0",
        "style-loader": "3.3.3",
        "swc-loader": "0.2.3",
        "terser-webpack-plugin": "5.3.9",
        "thread-loader": "4.0.2",
        "time-analytics-webpack-plugin": "0.1.20",
        "ts-node": "10.9.1",
        "typescript": "5.3.2",
        "vue-loader": "17.3.1",
        "webpack": "5.89.0",
        "webpack-bundle-analyzer": "4.10.1",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "4.15.1",
        "webpack-merge": "5.10.0",
        "webpack-sources": "3.2.3"
    },
    "dependencies": {
        "lodash-es": "4.17.21",
        "vue": "3.3.9"
    }
}
